
public class Employee 
	{
		String empId,empName,addr;
		
		public Employee() //Default Constructor
		{
			empId="E1001";
			empName="Suresh";
			addr="Hyderabad";
		}
		
		//Overloaded Constructor
		public Employee(String empid,String empname,String addr)
		{
			empId=empid;
			empName=empname;
			this.addr=addr;
		}
	}



